# Apache configuration for 404 redirect
ErrorDocument 404 /404.html

# Enable rewrite engine
<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Redirects for Deals
  RewriteRule ^go/FN-CFD-FUTURES-2024-11s9b$ https://www.fundednext.com/ [R=302,L]
  
  # If file or directory exists, serve it
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  
  # Otherwise, redirect to 404.html
  RewriteRule ^(.*)$ /404.html [L]
</IfModule>
